{
  "name": "ws_automation",
  "version": "1.0.0",
  "description": "WebSocket-based WhatsApp automation backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "author": "Emir Baktybekov",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.5.1",
    "@types/ws": "^8.5.5",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
